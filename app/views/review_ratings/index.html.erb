<h1>Listing review_ratings</h1>

<table>
  <tr>
    <th>Review</th>
    <th>Category</th>
    <th>City</th>
    <th>Rating</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @review_ratings.each do |review_rating| %>
  <tr>
    <td><%= review_rating.review %></td>
    <td><%= review_rating.category_id %></td>
    <td><%= review_rating.city_id %></td>
    <td><%= review_rating.rating %></td>
    <td><%= review_rating.user_id %></td>
    <td><%= link_to 'Show', review_rating %></td>
    <td><%= link_to 'Edit', edit_review_rating_path(review_rating) %></td>
    <td><%= link_to 'Destroy', review_rating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Review rating', new_review_rating_path %>
